import java.io.File;
import java.util.Scanner;
import java.util.ArrayList;


public class day7 {

	public static int howMuchFuel(ArrayList<Integer> pos, int target)
	{
		int fuelSpent = 0;
		
		//total the absolute value of the difference in target and position
		for(int i = 0; i < pos.size(); ++i)
		{
			fuelSpent += Math.abs(pos.get(i) - target);
		}
		
		return fuelSpent;
	}
	
	public static void main(String[] args) throws Exception {
		
		//declare variables
		File file = new File("C:\\Users\\isabel\\Downloads\\aocd7.txt");
        Scanner sc = new Scanner(file);
        ArrayList<Integer> crabList = new ArrayList<Integer>();
        String line = sc.next();
        int min = Integer.MAX_VALUE;
        int max = Integer.MIN_VALUE;
        
        String inputs[] = line.split(",");
        
        for (int i =0; i < inputs.length; ++i)
        {
        	//enter inputs to an array list
        	int value = Integer.parseInt(inputs[i]);
        	crabList.add(value);
        	
        	//update min and max values
        	if(value>max)
        	{
        		max = value;
        	}
        	else if(value<min)
        	{
        		min = value;
        	}
        }
       
        //cycle through each position from min to max to find the optimal one
        int minFuelUsage = Integer.MAX_VALUE;
        int fuelUsed;
        
        for (int i = min; i <= max; ++i)
        {
        	fuelUsed = howMuchFuel(crabList,i);
        	if(fuelUsed<minFuelUsage)
        	{
        		minFuelUsage = fuelUsed;
        	}
        }
        
        System.out.println(minFuelUsage);
		    	
		    	
		    sc.close();
	}

}
