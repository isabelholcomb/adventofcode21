import java.io.File;
import java.util.Scanner;

public class day1 {

	public static void main(String[] args) throws Exception {
        File file = new File("C:\\Users\\isabel\\Downloads\\aocd1.csv");
        Scanner sc = new Scanner(file);
        
        int cur = 0; //current integer
        int prev = 0; //previous integer
        int countDeeper =-1; //keeps track of whether the depth is increasing. starts at -1 to counterbalance the first depth
        
        while (sc.hasNextInt()) {
        	prev = cur; //update the previous value
        	cur = sc.nextInt(); //update the current
        	if(prev<cur)
        	{
        		countDeeper++;
        	}
        	
        }
        
        System.out.print(countDeeper);
        
        sc.close();
        
    }
